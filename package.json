{
  "name": "@linked-claims/koenig-civic-action-card",
  "version": "2.2.8",
  "description": "Civic Action card component for Ghost's Koenig editor (component only, node integration handled by Koenig)",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:client": "rollup -c rollup.config.client.js",
    "test:client": "node test-client-renderer.cjs",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@svgr/rollup": "^8.1.0",
    "jsdom": "^27.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^4.9.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "keywords": [
    "ghost",
    "koenig",
    "civic",
    "action",
    "card",
    "editor",
    "cooperation"
  ],
  "author": "Cooperation.org",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cooperation-org/koenig-civic-action-card"
  },
  "publishConfig": {
    "access": "public"
  }
}
